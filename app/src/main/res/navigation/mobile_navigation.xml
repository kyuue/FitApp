<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/mainMenuFragment">
    <fragment
        android:id="@+id/egitmenEkleFragment"
        android:name="com.example.fitapp.EgitmenEkleFragment"
        android:label="EgitmenEkleFragment" >
        <action
            android:id="@+id/action_egitmenEkleFragment_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainMenuFragment"
        android:name="com.example.fitapp.MainMenuFragment"
        android:label="MainMenuFragment" >
        <action
            android:id="@+id/action_mainMenuFragment_to_egitmenEkleFragment"
            app:destination="@id/egitmenEkleFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_egitmenSecFragment"
            app:destination="@id/egitmenSecFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_besinEkleFragment"
            app:destination="@id/besinEkleFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_kitleIndexFragment"
            app:destination="@id/kitleIndexFragment" />
    </fragment>
    <fragment
        android:id="@+id/egitmenSecFragment"
        android:name="com.example.fitapp.EgitmenSecFragment"
        android:label="EgitmenSecFragment" >
        <action
            android:id="@+id/action_egitmenSecFragment_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/besinEkleFragment"
        android:name="com.example.fitapp.BesinEkleFragment"
        android:label="BesinEkleFragment" >
        <action
            android:id="@+id/action_besinEkleFragment_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/kitleIndexFragment"
        android:name="com.example.fitapp.KitleIndexFragment"
        android:label="KitleIndexFragment" >
        <action
            android:id="@+id/action_kitleIndexFragment_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment" />
    </fragment>
</navigation>